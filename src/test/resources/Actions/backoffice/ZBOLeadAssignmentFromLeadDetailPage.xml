<suite name="Capture Lead From About Me Page" preserve-order="true" verbose="2" parallel="tests" thread-count="1">
	<listeners>
        <listener class-name="resources.ThreadCountChanger"/>
        <listener class-name="resources.ExtentReporterListener"/>
        <listener class-name="resources.ZurpleReporter.ScreenshotTaker"/>
        <listener class-name="resources.CustomTestNGReporter"/>
    </listeners>
   
	
<test name="Assignment of lead from Leads detail Page" >	
	<parameter name="addLeadData" value="/resources/data/zurple/backoffice/zurple-lead-assign-agent-data.json"> 
			<classes preserve-order="true">
				<class name="com.zurple.backoffice.ZBOLoginPageTest">
					<methods>
						<include name="testBackOfficeLogin" />
					</methods>
				</class>
				<class name="com.zurple.backoffice.ZBOAgentsPageTest">
					<methods>
						<include name="testVerifyLeadCount" />
					</methods>
				</class>
				<class name="com.zurple.backoffice.ZBOAddLeadPageTest">
					<methods>
						<include name="testAddLead" />
					</methods>
				</class>
				<class name="com.zurple.backoffice.ZBOLeadDetailPageTest">
					<methods>
						<include name="testUpdateLeadAgentAssignment" />
					</methods>
				</class>
				<class name="com.zurple.backoffice.ZBOLeadPageTest">
					<methods>
						<include name="testVerifyLeadAssignment" />
					</methods>
				</class>
				<class name="resources.CurrentPageTest">
					<methods>
						<include name="closeBrowser" />
					</methods>
				</class>
			</classes>
	</parameter>
	</test>
	
</suite>