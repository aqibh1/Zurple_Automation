<suite name="Create Ads Suite"
	preserve-order="true" verbose="2" parallel="tests" thread-count="1">
	<listeners>
		<listener class-name="resources.ThreadCountChanger" />
		<listener class-name="resources.ExtentReporterListener" />
		<listener
			class-name="resources.ZurpleReporter.ScreenshotTaker" />
		<listener class-name="resources.CustomTestNGReporter" />
	</listeners>


	<test name="Custom Ads Select Listings Verifications Step 1">
		<classes>
			<class name="com.zurple.backoffice.ZBOCreateAdPageTest">
				<methods>
					<include name="testVerifyListingPopUpAppearsOnClickingCreateCustomButton"/>
					<include name="testVerifyCorrectTextIsDisplayedOnSelectListingPopUp"/>
					<include name="testCancelButtonClosesThePopup"/>
				</methods>
			</class>
		</classes>
	</test>
</suite>