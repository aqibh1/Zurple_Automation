<suite name="Create Ads Suite"
	preserve-order="true" verbose="2" parallel="tests" thread-count="1">
	<listeners>
		<listener class-name="resources.ThreadCountChanger" />
		<listener class-name="resources.ExtentReporterListener" />
		<listener
			class-name="resources.ZurpleReporter.ScreenshotTaker" />
		<listener class-name="resources.CustomTestNGReporter" />
	</listeners>

	<test name="Create and Verify seller lead form ad">
	<parameter name="dataFile" value="/resources/data/zurple/ads/seller_lead/seller-lead-form-ad.json">
		<classes>
			<class name="com.zurple.backoffice.admin.ZACreateSellerLeadsAdPageTest">
				<methods>
					 <include name="testCreateAndVerifyLeadFormSellerLeadAd" />
				</methods>
			</class>
			<class name="com.zurple.backoffice.admin.ZAViewSellerLeadsAdPageTest">
				<methods>
					 <include name="testVerifyDateLeadFormSellerLeadAd" />
					 <include name="testVerifyAdTypeSellerLeadFormAd" />
					 <include name="testVerifyAdStatusSellerLeadFormAd" />
					 <include name="testVerifyBudgetSellerLeadFormAd" />
					 <include name="testVerifyCarouselAdStatusSellerLeadFormAd" />
					 <include name="testVerifyCarouselDownloadAdStatusSellerLeadFormAd" />
					 <include name="testVerifyCarouselVideoAdStatusSellerLeadFormAd" />
				</methods>
			</class>
		</classes>
		</parameter>
	</test>
</suite>