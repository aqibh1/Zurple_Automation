<suite name="Capture Lead From About Me Page"
	preserve-order="true" verbose="2" parallel="tests" thread-count="1">
	<listeners>
		<listener class-name="resources.ThreadCountChanger" />
		<listener class-name="resources.ExtentReporterListener" />
		<listener
			class-name="resources.ZurpleReporter.ScreenshotTaker" />
		<listener class-name="resources.CustomTestNGReporter" />
	</listeners>


	<test name="Verify priority Ranking for the lead">

		<parameter name="contactAgentData"
			value="/resources/data/zurple/register_user/lead-priority-ranking-data.json">
			<parameter name="dataFile"
				value="/resources/data/zurple/register_user/lead-priority-ranking-data.json">
				<classes>
					<class name="com.zurple.website.ZWPropertyDetailPageTest">
						<methods>
							<include name="testContactAgentOnListingDetail" />
						</methods>
					</class>
					<class name="com.zurple.backoffice.ZBOLoginPageTest">
						<methods>
							<include name="testBackOfficeLogin" />
						</methods>
					</class>
					<class name="com.zurple.backoffice.ZBOLeadCRMPageTest">
						<methods>
							<include name="testVerifyPriorityRanking" />
						</methods>
					</class>
					<class name="resources.CurrentPageTest">
						<methods>
							<include name="closeBrowser" />
						</methods>
					</class>
				</classes>
			</parameter>
		</parameter>
	</test>

</suite>