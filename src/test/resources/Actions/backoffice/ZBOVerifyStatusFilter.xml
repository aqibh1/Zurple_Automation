<suite name="Verify lead status filter" preserve-order="true"
	verbose="1" parallel="tests" thread-count="1">
	<listeners>
		<listener class-name="resources.ThreadCountChanger" />
		<listener class-name="resources.ExtentReporterListener" />
		<listener
			class-name="resources.ZurpleReporter.ScreenshotTaker" />
		<listener class-name="resources.CustomTestNGReporter" />
	</listeners>

	<test name="Verify lead status filter">
		<parameter name="dataFile"
			value="/resources/data/zurple/backoffice/status-filters-data.json">
			<classes>
				<class
					name="com.zurple.backoffice.ZBOLeadStatusFiltersPageTest">
					<methods>
						<include name="testLeadStatusFilter" />
					</methods>
				</class>
			</classes>
		</parameter>
	</test>
</suite>