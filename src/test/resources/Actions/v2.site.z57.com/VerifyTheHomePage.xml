<suite name="Search and Verify Home Page" preserve-order="true" verbose="2" parallel="tests" thread-count="1">
	<listeners>
        <listener class-name="resources.ThreadCountChanger"/>
    </listeners>
   
    <test name="Verify Home Page" preserve-order="true">
	<classes>
      <class name="com.z57.site.v2.HomePageTest" >
      <methods>
      	<include name="testVerifyHomePage" />
      </methods>
      </class>
        <class name="resources.CurrentPageTest">
        <methods>
          <include name="closeBrowser" />
        </methods>
      </class>
    </classes>
	</test>
	
</suite>