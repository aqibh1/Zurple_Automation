<suite name="Search And Verify Property suite"
	preserve-order="true" verbose="1" parallel="tests" thread-count="1">
	<listeners>
		<listener class-name="resources.ThreadCountChanger" />
		<listener
			class-name="resources.ZurpleReporter.ScreenshotTaker" />
	</listeners>

	<test
		name="Lead Capture Form appears for user not signed in on Listing">

		<parameter name="searchPropertyDataFile"
			value="/resources/data/zurple/property_search/zurple-property-search-lead-capture.json">

			<classes>

				<class name="com.zurple.website.ZWHomeSearchPageTest">
					<methods>
						<include name="testSearchPropoerty" />
					</methods>
				</class>
				<class name="com.zurple.website.ZWHomesForSalePageTest">
					<methods>
						<include name="testHomesForSale" />
					</methods>
				</class>
				<class name="com.zurple.website.ZWPropertyDetailPageTest">
					<methods>
						<include name="testVerifyPropertyDetails" />
					</methods>
				</class>
				<class name="resources.CurrentPageTest">
					<methods>
						<include name="closeBrowser" />
					</methods>
				</class>
			</classes>

		</parameter>
	</test>
</suite>