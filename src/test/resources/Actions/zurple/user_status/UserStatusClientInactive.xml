<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >

<suite name="User Not Change Status From Client to Inactive">

  <test name="Trigger user status change client->inactive">
    <parameter name="status" value="inactive"/>
    <classes>
      <class name="com.zurple.my.LeadDetailPageTest">
        <methods>
          <include name="testLeadStatusUpdate" />
        </methods>
      </class>
    </classes>
  </test>
  

  <test name="Check user status is CLIENT">
    <parameter name="status_expected" value="inactive"/>
    <classes>
      <class name="resources.models.UserTest">
        <methods>
          <include name="testLastUserStatus" />
        </methods>
      </class>
    </classes>
  </test>

</suite>
