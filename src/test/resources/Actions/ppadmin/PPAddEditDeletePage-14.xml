      <methods>
      	<include name="testPPLogin" />
      </methods>
      </class>
      <class name="com.z57.propertypulse.PPAddNewPageTest" >
      <methods>
      	<include name="testAddNewPage" />
      </methods>
      </class>
       <class name="com.z57.propertypulse.PPAddNewPageTest" >
      <methods>
      	<include name="testEditPage" />
      </methods>
      </class>
      <class name="resources.CurrentPageTest">
      <methods>
         <include name="closeBrowser" />
      </methods>
      </class>
    </classes>
    </parameter>
	</test>
	
</suite>