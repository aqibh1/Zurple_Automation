<suite name="Capture Lead From About Me Page"
	preserve-order="true" verbose="1" parallel="tests" thread-count="1">
	<listeners>
		<listener class-name="resources.ThreadCountChanger" />
		<listener class-name="resources.ExtentReporterListener" />
		<listener
			class-name="resources.ZurpleReporter.ScreenshotTaker" />
		<listener class-name="resources.CustomTestNGReporter" />
	</listeners>


	<test name="Add Listing and Verify Accelator triggered">
	  
		<parameter name="dataFile"
			value="/resources/data/propertypulse/listings/listing-accelator-posts-data-02.json">
			<classes>
			
				<class name="com.z57.propertypulse.LoginPageTest">
					<methods>
						<include name="testPPLogin" />
					</methods>
				</class>
				<class name="com.z57.propertypulse.PPListingPageTest">
					<methods>
						<include name="testAddListing" />
					</methods>
				</class>
				<class name="com.z57.propertypulse.PPListingDetailPageTest">
					<methods>
						<include name="testEditListingDetails" />
					</methods>
				</class>
		
				<class name="com.z57.propertypulse.DatabasePageTest">
					<methods>
						<include name="testVerifyCronIsTriggered" />
					</methods>
				</class>
				<class name="resources.CurrentPageTest">
					<methods>
						<include name="closeBrowser" />
					</methods>
				</class>
			</classes>
		</parameter>

	</test>

</suite>