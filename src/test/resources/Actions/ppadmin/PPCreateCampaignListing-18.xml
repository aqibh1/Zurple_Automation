      </class>
      <class name="com.z57.propertypulse.PPEmailCampaignManagerPageTest" >
      <methods>
      	<include name="testEmailCampaignManagerListing" />
      </methods>
      </class>
      <class name="resources.CurrentPageTest">
      <methods>
         <include name="closeBrowser" />
      </methods>
      </class>
    </classes>
    </parameter>
	</test>
	
</suite>