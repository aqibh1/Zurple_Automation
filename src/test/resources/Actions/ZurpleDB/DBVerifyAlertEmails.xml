<suite name="Verify Alert emails" preserve-order="true"
	verbose="1" parallel="tests" thread-count="1">
	<listeners>
		<listener class-name="resources.ThreadCountChanger" />
		<listener class-name="resources.ExtentReporterListener" />
		<listener
			class-name="resources.ZurpleReporter.ScreenshotTaker" />
		<listener class-name="resources.CustomTestNGReporter" />
	</listeners>

	<test name="Verify Alert emails from DB">
			<classes>
				<class name="com.zurple.backoffice.ZBOAlertsPageDBTest">
					<methods>
						<include name="testScheduleAlertFromDB" />
						<include name="testAgentInquiryAlertFromDB" /> 
						<include name="testCheckinAlertFromDB" /> 
						<include name="testCMAAlertFromDB" /> 
						<include name="testCMAFollowUpAlertFromDB" /> 
						<include name="testCommunitySearchAlertFromDB" /> 
						<include name="testDownloadSellerAlertFromDB" /> 
						<include name="testFavoritesAlertFromDB" /> 
						<include name="testHighActivityAlertFromDB" /> 
						<include name="testHighAppreciationFromDB" /> 
						<include name="testHighReturnAlertFromDB" /> 
						<include name="testHiighValueAlertFromDB" /> 
						<include name="testImportedLeadAlertFromDB" /> 
						<include name="testNewSignUpLeadAlertFromDB" /> 
						<include name="testNextDayFollowUpLeadAlertFromDB" /> 
						<include name="testPOILeadAlertFromDB" /> 
						<include name="testPreferredPropertyLeadAlertFromDB" /> 
						<include name="testPropertyInquiryLeadAlertFromDB" /> 
						<include name="testSchoolReportAlertFromDB" /> 
						<include name="testUserReplyAlertFromDB" /> 
						<include name="testSellerInquiryAlertFromDB" /> 
