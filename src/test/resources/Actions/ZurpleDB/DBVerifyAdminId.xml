<suite name="Verify Admin id from Users table" preserve-order="true"
	verbose="1" parallel="tests" thread-count="1">
	<listeners>
		<listener class-name="resources.ThreadCountChanger" />
		<listener class-name="resources.ExtentReporterListener" />
		<listener class-name="resources.ZurpleReporter.ScreenshotTaker" />
		<listener class-name="resources.CustomTestNGReporter" />
	</listeners>

	<test name="Verify Admin id is assigned correctly to leads">
			<classes>
				<class name="com.zurple.database.ZDBAdminIdTest">
					<methods>
						<include name="testVerifyAdminIdFromUsersTable" />
					</methods>
				</class>
			</classes>
	</test>
</suite>